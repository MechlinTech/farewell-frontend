{
  "extends": "./node_modules/@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@color": ["./styles/color.tsx"],
      "@dimensions": ["./styles/dimensions.tsx"],
      "@client": ["./api/client.tsx"],
      "@queries": ["./api/queries.tsx"],
      "@AsyncStoreHelper": ["./utils/AsyncStoreHelper.tsx"],
      "@constants": ["./styles/constants.tsx"],
      "@Navigator": ["./utils/Navigator.ts"],
      "@Utils": ["./utils/Utils.js"],
      "@scale": ["./utils/scale.ts"],
      "@Helper": ["./src/utils/Helper.tsx"],
      "@images": ["./assets/images/index.ts"],
      "@components/*": ["./components/*"],
      "@redux/*": ["./redux/*"],
      "@screens/*": ["./screens/*"],
      "@api/*": ["./api/*"],
      "react-native": ["./node_modules/@types/react-native"]
    }
  }
}
